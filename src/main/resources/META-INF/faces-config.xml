<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<name>jainfaces</name>

	<ordering>
		<after>
			<name>primefaces</name>
		</after>
	</ordering>

	<component>
		<component-type>com.jain.faces.component.JNLayout</component-type>
		<component-class>com.jain.faces.layout.JNLayout</component-class>
	</component>

	<component>
		<component-type>com.jain.faces.component.JNLayoutPane</component-type>
		<component-class>com.jain.faces.layout.JNLayoutPane</component-class>
	</component>

	<component>
		<component-type>com.jain.faces.component.JNGrid</component-type>
		<component-class>com.jain.faces.grid.JNGrid</component-class>
	</component>

	<component>
		<component-type>com.jain.faces.component.JNGroup</component-type>
		<component-class>com.jain.faces.group.JNGroup</component-class>
	</component>
	
	<component>
		<component-type>com.jain.faces.component.JNOutput</component-type>
		<component-class>com.jain.faces.output.JNOutput</component-class>
	</component>


	<render-kit>
		<renderer>
			<component-family>com.jain.faces.component</component-family>
			<renderer-type>com.jain.faces.component.JNLayoutRenderer</renderer-type>
			<renderer-class>com.jain.faces.layout.JNLayoutRenderer</renderer-class>
		</renderer>

		<renderer>
			<component-family>com.jain.faces.component</component-family>
			<renderer-type>com.jain.faces.component.JNGridRenderer</renderer-type>
			<renderer-class>com.jain.faces.grid.JNGridRenderer</renderer-class>
		</renderer>

		<renderer>
			<component-family>com.jain.faces.component</component-family>
			<renderer-type>com.jain.faces.component.JNGroupRenderer</renderer-type>
			<renderer-class>com.jain.faces.group.JNGroupRenderer</renderer-class>
		</renderer>
		
		<renderer>
			<component-family>com.jain.faces.component</component-family>
			<renderer-type>com.jain.faces.component.JNOutputRenderer</renderer-type>
			<renderer-class>com.jain.faces.output.JNOutputRenderer</renderer-class>
		</renderer>

	</render-kit>


</faces-config>